{
    "name": "lowel/docker-api-php-sdk",
    "description": "Docker Engine API PHP SDK",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Lowel\\Docker\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Andrey Bulavskiy",
            "email": "10vvvvvv31@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "guzzlehttp/guzzle": "^7.8",
        "ext-curl": "*",
        "symfony/http-foundation": "^7.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.16"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
